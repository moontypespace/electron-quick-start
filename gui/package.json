{
  "name": "FontDev",
  "version": "0.0.1",
  "description": "A font post production application using python.",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Fonts",
    "Development",
    "Engineering",
    "Tables",
    "Glyphs",
    "Diff"
  ],
  "author": "Olli Meier",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "python-shell": "^3.0.1"
  },
  "build": {
    "mac": {
      "icon": "./myicon.icns",
      "category": "public.app-category.developer-tools"
    },
    "extraResources": [
      {
        "from": "python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      },
            {
        "from": "engine",
        "to": "engine",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
