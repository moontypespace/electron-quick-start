{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "python-shell": "^3.0.1",
    "sweetalert2": "^11.4.0"
  },
  "build": {
    "extraResources": [
      {
        "from": "python",
        "to": "python",
        "filter": [
          "**/*"
        ]
      },
            {
        "from": "engine",
        "to": "engine",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
